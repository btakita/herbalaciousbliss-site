{
	"name": "@btakita/herbaliciousbliss-site",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"build": "bun build.ts",
		"clear": "rm -rf ./dist",
		"dev": "bun run dev:sourcemap__resolve__bun",
		"dev:raw": "bun index.ts",
		"dev:sourcemap__resolve__bun": "bin/sourcemap__resolve__bun.ts -f index.ts",
		"db:drop": "drizzle-kit drop",
		"db:generate": "drizzle-kit generate:sqlite",
		"db:migrate": "bun ./db/migrate.ts",
		"db:push": "drizzle-config push:sqlite",
		"start": "bin/sourcemap__resolve__bun.ts -f start.ts",
		"clean": "rm -rf ./dist"
	},
	"dependencies": {
		"@ctx-core/preprocess": "^0.1.3",
		"@ctx-core/source-map": "^0.2.108",
		"@btakita/domain--server--herbaliciousbliss": "workspace:*",
		"@btakita/ui--any--herbaliciousbliss": "workspace:*",
		"@btakita/ui--server--herbaliciousbliss": "workspace:*",
		"@rappstack/domain--any--blog": "workspace:*",
		"@rappstack/domain--server": "workspace:*",
		"@rappstack/domain--server--blog": "workspace:*",
		"@rappstack/ui--any": "workspace:*",
		"@rappstack/ui--any--blog": "workspace:*",
		"@rappstack/ui--browser--blog": "workspace:*",
		"@rappstack/ui--server": "workspace:*",
		"@rappstack/ui--server--blog": "workspace:*",
		"@rebuildjs/tailwindcss": "^0.10.45",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.13",
		"arctic": "^1.9.1",
		"cssnano": "^7.0.3",
		"ctx-core": "^7.2.25",
		"drizzle-kit": "^0.22.7",
		"drizzle-orm": "^0.31.2",
		"elysia": "^1.0.25",
		"esbuild": "^0.21.5",
		"esmcss": "^0.2.11",
		"marked": "^13.0.1",
		"nodemailer": "^6.9.14",
		"oslo": "^1.2.1",
		"picocolors": "^1.0.1",
		"rebuildjs": "^0.70.34",
		"relementjs": "^0.80.24",
		"relysjs": "^1.21.60",
		"tailwindcss": "^3.4.4"
	},
	"devDependencies": {
		"bun-types": "latest"
	}
}